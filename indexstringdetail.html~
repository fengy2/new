		function test1(){
			$.ajax({
				 type: "GET",
				 url:"http://localhost:8080/jsonstring",
				 //data: {username:$("#username").val(), content:$("#content").val()},
				 dataType: "json",
				 success: function(data){
					//var temp = "<tr class="odd gradeX"><td>Trident</td><td>InternetExplorer 4.0</td><td>Win 95+</td><td class="center"> 4</td><td class="center">X</td></tr>";
					var html = '';
					html=''
					//[{"key":"string","Pre":"String","value":"11"},{}]
					for(var i=0;i<data.length;i++){
						html += '<tr class="odd gradeX"><td>'+ data[i].pre +'</td><td>'+ data[i].key +'</td><td>'+ data[i].value +'</td><td class="center">dash</td><td class="center">dash</td></tr>';	
						
					}
					
					 
					$('#str_table').html(html);
				//alert(data);
				
				//var z = eval(data);
				console.log(data);
				console.log(data[0].pre);
				},
				error:function(a){
					alert("error");
				}
			});
		}


// db0的key总数
